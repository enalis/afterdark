:[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"featType":"classFeat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Eldritch Archer: Arcana - Wave Step"},"sort":4100000,"flags":{},"img":"https://assets.forge-vtt.com/5f4b3f12a7f7e16e4013bc80/bf72bfa2c90decad796489f9c7c9eea0.jpg","effects":[]},{"_id":"I3DFEkm6t87uCyM3","name":"Eldritch Archer: Knowledge Pool","type":"feat","data":{"description":{"value":"<p><span id=\"docs-internal-guid-e5b459b5-7fff-a9f1-08bb-621f65ff45a9\"><span style=\"font-size: 12pt; font-family: Arial; color: #333333; font-variant-numeric: normal; font-variant-east-asian: normal; vertical-align: baseline; white-space: pre-wrap;\">At 7th level, when a magus prepares his magus spells, he can decide to expend 1 or more points from his arcane pool, up to his </span><a style=\"text-decoration-line: none;\" href=\"https://www.d20pfsrd.com/basics-ability-scores/ability-scores#TOC-Intelligence-Int-\"><span style=\"font-size: 12pt; font-family: Arial; color: #1155cc; font-variant-numeric: normal; font-variant-east-asian: normal; text-decoration-line: underline; text-decoration-skip-ink: none; vertical-align: baseline; white-space: pre-wrap;\">Intelligence</span></a><span style=\"font-size: 12pt; font-family: Arial; color: #333333; font-variant-numeric: normal; font-variant-east-asian: normal; vertical-align: baseline; white-space: pre-wrap;\"> bonus. For each point he expends, he can treat any one spell from the magus spell list as if it were in his spellbook and can prepare that spell as normal that day. If he does not cast spells prepared in this way before the next time he prepares spells, he loses those spells. He can also cast spells added in this way using his spell recall ability, but only until he prepares spells again.</span></span></p>\n<p>&nbsp;</p>\n<p><strong><span style=\"font-size: 12pt; font-family: Arial; color: #333333; font-variant-numeric: normal; font-variant-east-asian: normal; vertical-align: baseline; white-space: pre-wrap;\">Mythic</span></strong></p>\n<p><span style=\"font-size: 12pt; font-family: Arial; color: #333333; font-variant-numeric: normal; font-variant-east-asian: normal; vertical-align: baseline; white-space: pre-wrap;\">The magus may use the knowledge pool ability with wizard spells, as well as magus spells.</span></p>","chat":"","unidentified":""},"tags":[],"activation":{"cost":1,"type":""},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","long":null},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"featType":"classFeat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Eldritch Archer: Knowledge Pool"},"sort":4100000,"flags":{},"img":"https://assets.forge-vtt.com/5f4b3f12a7f7e16e4013bc80/bf72bfa2c90decad796489f9c7c9eea0.jpg","effects":[]},{"_id":"SSprwyX1v2CIizWY","name":"Eldritch Archer: Ranged Weapon Bond","type":"feat"